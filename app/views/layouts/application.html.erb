<!DOCTYPE html>
<html>
  <head>
    <title>Rails1</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <!--%= render "layouts/nav_bar"%-->

  <% unless session[:sampleform_id].nil?%>
  <%= link_to "Logout", logout_users_path, method: :delete%> 
  <%else%>
  <%= link_to "signin", sign_in_users_path %>
  <%end%>
  <%flash.each do |_,value|%>
  <%=value%>
  <%end%>
    <%= yield %>
  </body>
</html>
