<%= form_for @user do |f|%>
<table>
  <tr>
  <td><%= f.label :name%></td>
  <td><%= f.text_field :name%></td>
  </tr>
  <tr>
  <td><%= f.label :mail%></td>
  <td><%= f.email_field :mail%></td>
  </tr>
  <tr>
  <td><%= f.label :password%></td>
  <td><%= f.password_field :password%></td>
  </tr>
  <tr>
  <td><%= f.label :mobile%></td>
  <td><%= f.text_field :mobile%></td>
  </tr>
  <tr>
  <td><%= f.label :address%></td>
  <td><%= f.text_field :address%></td>
  </tr>
    <tr>
  <td><%= f.submit "Save"%></td>
  </tr>
</table>
<%end%>

